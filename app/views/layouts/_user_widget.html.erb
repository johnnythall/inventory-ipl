<% if user_signed_in? %>
  <h3><%= current_user[:first_name].to_s + " " + current_user[:last_name].to_s %></h3><p><%= current_user[:email] %></p>
  <%= link_to 'Logout', destroy_user_session_path, :method => :delete %>
<% else %>
  <p>You are not signed in.</p>
  <%= link_to 'Login', new_user_session_path %>
<% end %>